{
  "url": "https://www.theguardian.com/world/all",
  "properties": {
    "news_stories": {
      "type": "array",
      "selector_type": "css",
      "selector": "li.fc-slice__item, article, .js-headline-list-item, .u-faux-block-link",
      "items": {
        "properties": {
          "title": {
            "selector": "h2, h3, h4, .js-headline-text, .fc-item__title",
            "attribute": null
          },
          "link": {
            "selector": "a.fc-item__link, a.u-faux-block-link__overlay",
            "attribute": "href"
          },
          "description": {
            "selector": "p, .fc-item__standfirst",
            "attribute": null
          },
          "date": {
            "selector": "time, .fc-item__timestamp, .fc-item__meta",
            "attribute": null
          }
        }
      }
    }
  },
  "actions": [
    {
      "type": "wait",
      "duration": 3.0
    }
  ]
}
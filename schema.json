{
  "url": "https://wiflix-pro.mom/film-en-streaming/32799-captain-america-brave-new-world.html",
  "properties": {
    "title": {
      "type": "string",
      "selector_type": "css",
      "selector": "h1.heading-1"
    },
    "description": {
      "type": "string",
      "selector_type": "css",
      "selector": ".synopsis"
    },
    "streaming_servers": {
      "type": "array",
      "selector_type": "css",
      "selector": ".tabs-sel li",
      "items": {
        "properties": {
          "name": {
            "selector": "self",
            "type": "string"
          }
        }
      }
    },
    "page_html": {
      "type": "string",
      "selector_type": "css",
      "selector": "html",
      "attribute": "innerHTML"
    }
  },
  "actions": [
    {
      "type": "wait",
      "duration": 3
    },
    {
      "type": "click",
      "selector_type": "css",
      "selector": ".tabs-sel li:nth-child(1)",
      "retries": 3
    },
    {
      "type": "wait",
      "duration": 2
    }
  ],
  "post_actions": {
    "iframe_sources": {
      "type": "regex",
      "selector_type": "css",
      "selector": "script",
      "pattern": "url_iframe = ['\"]([^'\"]+)['\"]"
    },
    "direct_player_urls": {
      "selector_type": "css",
      "selector": "iframe",
      "attribute": "src"
    },
    "all_streaming_links": {
      "type": "network",
      "methods": ["GET", "POST"],
      "pattern": "embed|player|stream|iframe|video"
    }
  }
}
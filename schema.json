{
  "url": "https://www.amazon.com/s?k=laptops&ref=nb_sb_noss_2",
  "properties": {
    "products": {
      "type": "array",
      "selector": "div.s-main-slot.s-result-list.s-search-results.sg-row > div[data-component-type='s-search-result']",
      "items": {
        "properties": {
          "name": {
            "type": "string",
            "selector": "h2 a.a-link-normal.a-text-normal",
            "attribute": "innerText"
          },
          "price": {
            "type": "string",
            "selector": "span.a-price > span.a-offscreen",
            "attribute": "innerText"
          },
          "rating": {
            "type": "string",
            "selector": "span.a-icon-alt",
            "attribute": "innerText"
          },
          "url": {
            "type": "string",
            "selector": "h2 a.a-link-normal.a-text-normal",
            "attribute": "href"
          }
        }
      }
    }
  },
  "actions": [
    {
      "type": "wait",
      "duration": 5
    }
  ],
  "post_actions": {
    "clean_data": {
      "type": "network",
      "pattern": ".*amazon.com.*",
      "methods": ["GET"],
      "capture_response": true
    }
  },
  "headers": {
    "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"
  },
  "max_retries": 3,
  "timeout": 60
}
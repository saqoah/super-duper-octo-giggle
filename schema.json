{
  "url": "https://www.amazon.com/best-sellers-laptops/zgbs/pc/565108",
  "properties": {
    "laptops": {
      "type": "array",
      "selector_type": "css",
      "selector": "div.zg-grid-general-faceout",
      "items": {
        "properties": {
          "rank": {
            "type": "string",
            "selector": "span.zg-bdg-text",
            "attribute": "textContent"
          },
          "title": {
            "type": "string",
            "selector": "div > a > span",
            "attribute": "textContent"
          },
          "link": {
            "type": "string",
            "selector": "div > a",
            "attribute": "href"
          },
          "price": {
            "type": "string",
            "selector": "span._cDEzb_p13n-sc-price_3mJ9Z",
            "attribute": "textContent"
          },
          "rating": {
            "type": "string",
            "selector": "span[aria-label*='out of 5 stars']",
            "attribute": "aria-label"
          },
          "review_count": {
            "type": "string",
            "selector": "span.a-size-small",
            "attribute": "textContent"
          },
          "image": {
            "type": "string",
            "selector": "img",
            "attribute": "src"
          }
        },
        "filter": {
          "attribute": "title",
          "pattern": ".{10,}"
        }
      }
    }
  },
  "post_actions": {
    "network_requests": {
      "type": "network",
      "methods": ["GET"],
      "pattern": ".*(?:json|api|product).*"
    }
  }
}
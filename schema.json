{
  "url": "https://direct-streamfr.live/player.php?id=82",
  "properties": {
    "inner_text": {
      "type": "string",
      "selector_type": "css",
      "selector": "body"
    },
    "playback_urls": {
      "type": "regex",
      "selector_type": "css",
      "selector": "script, iframe",
      "pattern": "\"(https?://[^\"]+?(?:embed|video|stream|media|player|m3u8|mp4|ts)[^\"]*)\""
    }
  },
  "post_actions": {
    "iframe_urls": {
      "selector_type": "css",
      "selector": "iframe",
      "attribute": "src"
    },
    "network_requests": {
      "type": "network",
      "methods": ["GET", "POST"],
      "pattern": ".*\\.(m3u8|mp4)$"
    }
  }
}
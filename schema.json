{
  "url": "https://www.bbc.com/news/world/europe",
  "properties": {
    "title": {
      "type": "string",
      "selector": "title"
    },
    "description": {
      "type": "string",
      "selector": "meta[name='description']",
      "attribute": "content"
    },
    "images": {
      "type": "array",
      "selector": "img",
      "items": {
        "properties": {
          "src": {
            "type": "string",
            "selector": "img",
            "attribute": "src",
            "is_image": true
          },
          "alt": {
            "type": "string",
            "selector": "img",
            "attribute": "alt"
          }
        }
      }
    },
    "videos": {
      "type": "array",
      "selector": "video",
      "items": {
        "properties": {
          "src": {
            "type": "string",
            "selector": "video",
            "attribute": "src"
          }
        }
      }
    },
    "body": {
      "type": "string",
      "selector": "body"
    },
    "social_media": {
      "type": "array",
      "selector": "a",
      "items": {
        "properties": {
          "youtube": {
            "type": "string",
            "selector": "a[href*='youtube.com']",
            "attribute": "href"
          },
          "twitter": {
            "type": "string",
            "selector": "a[href*='twitter.com']",
            "attribute": "href"
          },
          "facebook": {
            "type": "string",
            "selector": "a[href*='facebook.com']",
            "attribute": "href"
          },
          "instagram": {
            "type": "string",
            "selector": "a[href*='instagram.com']",
            "attribute": "href"
          }
        }
      }
    },
    "emails": {
      "type": "array",
      "selector": "a[href^='mailto:']",
      "items": {
        "properties": {
          "email": {
            "type": "string",
            "selector": "a[href^='mailto:']",
            "attribute": "href"
          }
        }
      }
    },
    "phone_numbers": {
      "type": "array",
      "selector": "a[href^='tel:']",
      "items": {
        "properties": {
          "phone_number": {
            "type": "string",
            "selector": "a[href^='tel:']",
            "attribute": "href"
          }
        }
      }
    },
    "links": {
      "type": "array",
      "selector": "a[href]",
      "items": {
        "properties": {
          "url": {
            "type": "string",
            "selector": "a[href]",
            "attribute": "href"
          }
        }
      }
    }
  }
}

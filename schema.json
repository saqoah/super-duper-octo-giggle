{
  "url": "https://www.bbc.com/",
  "properties": {
    "title": {
      "type": "string",
      "selector": "title"
    },
    "description": {
      "type": "string",
      "selector": "meta[name='description']",
      "attribute": "content"
    },
    "images": {
      "type": "array",
      "selector": "img",
      "items": {
        "properties": {
          "src": {
            "type": "string",
            "selector": "img",
            "attribute": "src",
            "is_image": true
          },
          "alt": {
            "type": "string",
            "selector": "img",
            "attribute": "alt"
          }
        }
      }
    },
    "body": {
      "type": "string",
      "selector": "body"
    }
  }
}
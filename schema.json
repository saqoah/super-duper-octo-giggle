{
  "url": "https://french-stream.my/15119949-the-gorge.html",
  "properties": {
    "title": {
      "type": "string",
      "selector": "title",
      "selector_type": "css"
    },
    "description": {
      "type": "string",
      "selector": "meta[name='description']",
      "selector_type": "css",
      "attribute": "content"
    },
    "images": {
      "type": "array",
      "selector": "img",
      "selector_type": "css",
      "items": {
        "properties": {
          "src": {
            "selector": "self",
            "selector_type": "css",
            "attribute": "src"
          },
          "alt": {
            "selector": "self",
            "selector_type": "css",
            "attribute": "alt"
          }
        }
      }
    },
    "uqloadLinks": {
      "type": "regex",
      "pattern": "uqload[\\w.-/]+",
      "selector_type": "css",
      "selector": "a, iframe"
    }
  }
}
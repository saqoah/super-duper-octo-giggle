{
  "url": "https://www.theguardian.com/uk",
  "properties": {
    "news_articles": {
      "type": "array",
      "selector_type": "css",
      "selector": "a[href*='/20']",
      "items": {
        "properties": {
          "headline": {
            "type": "string",
            "selector": "self",
            "attribute": null
          },
          "url": {
            "type": "string",
            "selector": "self",
            "attribute": "href"
          }
        }
      }
    },
    "article_headlines": {
      "type": "array",
      "selector_type": "css",
      "selector": "h1, h2, h3",
      "items": {
        "properties": {
          "text": {
            "type": "string",
            "selector": "self",
            "attribute": null
          }
        }
      }
    },
    "article_images": {
      "type": "array",
      "selector_type": "css",
      "selector": "img[srcset], img[src*='guim']",
      "items": {
        "properties": {
          "src": {
            "type": "string",
            "selector": "self",
            "attribute": "src"
          },
          "alt": {
            "type": "string",
            "selector": "self",
            "attribute": "alt"
          }
        }
      }
    },
    "nav_sections": {
      "type": "array",
      "selector_type": "css",
      "selector": "nav a",
      "items": {
        "properties": {
          "section_name": {
            "type": "string",
            "selector": "self",
            "attribute": null
          },
          "section_url": {
            "type": "string",
            "selector": "self",
            "attribute": "href"
          }
        }
      }
    }
  },
  "actions": [
    {
      "type": "wait",
      "duration": 5.0
    },
    {
      "type": "click",
      "selector": "button[data-link-name='dismiss']",
      "retries": 1
    }
  ],
  "post_actions": {
    "meta_title": {
      "type": "string",
      "selector_type": "css",
      "selector": "title",
      "attribute": null
    },
    "meta_description": {
      "type": "string",
      "selector_type": "css",
      "selector": "meta[name='description']",
      "attribute": "content"
    },
    "json_ld": {
      "type": "string",
      "selector_type": "css",
      "selector": "script[type='application/ld+json']",
      "attribute": "innerHTML"
    }
  }
}
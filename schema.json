{
  "url": "https://egydead.fyi/episode/%d9%85%d8%b3%d9%84%d8%b3%d9%84-reacher-%d8%a7%d9%84%d9%85%d9%88%d8%b3%d9%85-%d8%a7%d9%84%d8%ab%d8%a7%d9%84%d8%ab-%d8%a7%d9%84%d8%ad%d9%84%d9%82%d8%a9-3-%d9%85%d8%aa%d8%b1%d8%ac%d9%85%d8%a9/",
  "properties": {
    "page_title": {
      "type": "string",
      "selector_type": "css",
      "selector": "title"
    },
    "playback_urls": {
      "type": "regex",
      "selector_type": "css",
      "selector": "script, iframe",
      "pattern": "\"(https?://[^\"]+?(?:embed|video|stream|media|player|m3u8|mp4|ts)[^\"]*)\""
    }
  },
  "actions": [
    {
      "type": "click",
      "selector_type": "css",
      "selector": "body",
      "retries": 1
    },
    {
      "type": "wait",
      "duration": 5
    }
  ],
  "post_actions": {
    "iframe_urls": {
      "selector_type": "css",
      "selector": "iframe",
      "attribute": "src"
    },
    "network_requests": {
      "type": "network",
      "methods": ["GET", "POST"],
      "pattern": ".*\\.(m3u8|mp4)$"
    }
  }
}
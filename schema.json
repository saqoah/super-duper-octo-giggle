{
  "url": "https://www.bbc.com",
  "properties": {
    "title": {
      "type": "string",
      "selector": "title",
      "selector_type": "css"
    },
    "description": {
      "type": "string",
      "selector": "meta[name='description']",
      "selector_type": "css",
      "attribute": "content"
    },
    "images": {
      "type": "array",
      "selector": "img",
      "selector_type": "css",
      "items": {
        "properties": {
          "src": {
            "selector": "self",
            "selector_type": "css",
            "attribute": "src"
          },
          "alt": {
            "selector": "self",
            "selector_type": "css",
            "attribute": "alt"
          }
        }
      }
    },
    "bodyText": {
      "type": "string",
      "selector": "body",
      "selector_type": "css"
    }
  }
}